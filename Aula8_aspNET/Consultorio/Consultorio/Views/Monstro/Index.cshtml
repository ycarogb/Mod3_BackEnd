@{
}

<table class="table">
    <thead>
        <tr>
            <th scope="col">Id</th>
            <th scope="col">Nome</th>
        </tr>
    </thead>
    <tbody>
        @foreach (Monstro m in ViewBag.monstros)
        {
            <tr>
                <th scope="row">@m.Id</th>
                <td>@m.Nome</td>
            </tr>
        }
    </tbody>
</table>